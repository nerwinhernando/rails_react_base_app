{
  "name": "rails_react_base_app",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "3.5",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "3.3.7",
    "chartist": "0.10.1",
    "history": "4.7.2",
    "moment": "2.21.0",
    "node-sass": "4.6.1",
    "node-sass-chokidar": "0.0.3",
    "nouislider": "11.0.3",
    "npm-run-all": "4.1.2",
    "perfect-scrollbar": "1.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-big-calendar": "0.19.0",
    "react-bootstrap": "0.32.1",
    "react-bootstrap-sweetalert": "4.2.3",
    "react-bootstrap-switch": "15.5.3",
    "react-chartist": "0.13.1",
    "react-datetime": "2.14.0",
    "react-dom": "^16.6.1",
    "react-google-maps": "9.4.5",
    "react-jvectormap": "0.0.2",
    "react-notification-system": "0.2.17",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.1",
    "react-select": "1.2.1",
    "react-stepzilla": "4.7.0",
    "react-syntax-highlighter": "7.0.2",
    "react-table": "6.8.0",
    "react-tagsinput": "3.19.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "webpack-cli": "^3.1.2",
    "wnumb": "1.1.0"
  },
  "devDependencies": {
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "postcss": "^7.0.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "webpack-dev-server": "^2.11.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./app/javascript/packs/src --include-path ./node_modules app/javascript/packs/src/ -o app/javascript/packs/src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./app/javascript/packs/src --include-path ./node_modules app/javascript/packs/src/ -o app/javascript/packs/src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
